{% extends 'base.html.twig' %}
{% block meta_title %}Home{% endblock meta_title %}
{% block main_book %}
        <div class="home_book">
            {% for book in books %}
                <div class="home_book">
                    <div class="homed_book">
                        <h1 class="h1_list">{{ book.title }}</h1>
                        <p class="author_list">{{ book.author }}</p>
                    </div>
                </div>
            {% endfor %}
        </div>
 {% endblock main_book %}